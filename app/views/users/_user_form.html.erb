<%= form_for user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br />
  <%= f.label :email %>
  <%= f.text_field :email %><br />
  <%= f.label :password %>
  <%= f.password :password %><br />
  <%= f.label :password_confirmation %>
  <%= f.password :password_confirmation %><br />
  <%= f.submit %>
<% end %>




